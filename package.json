{
  "name": "nativetable",
  "version": "1.0.0",
  "description": "Create and use dynamics HTML tables with native JS",
  "repository": "https://git.nathanaelcherrier.com/mindsers/nativetable.git",
  "main": "src/scripts/build/nativetable.min.js",
  "scripts": {
    "build:js": "rollup -c",
    "build:clean": "rm -rf src/scripts/build",
    "build:dev": "npm run build:clean && npm run build:js -- --sourcemap -w",
    "build:prod": "npm run build:clean && npm run build:js",
    "build": "npm run build:prod",
    "test:coverage": "isparta cover _mocha -- -r babel-core/register -R progress",
    "test:unit": "mocha -r babel-core/register",
    "test:clean": "rm -rf coverage",
    "test": "npm run test:clean && npm run test:unit && npm run test:coverage"
  },
  "keywords": [
    "table",
    "filter"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0"
  },
  "dependencies": {}
}
